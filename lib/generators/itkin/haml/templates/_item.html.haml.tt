%li[<%=singular_name%>]
  %h2
    = render :partial => 'actions', :locals => { :<%=singular_name%> => <%=singular_name%> }
    = link_to "#" do
      <%- for attribute in attributes -%>
      = <%=singular_name%>.<%=attribute.name%>
      <%- end -%>
  %div
    = flash_message
    = render :partial => <%=singular_name%>